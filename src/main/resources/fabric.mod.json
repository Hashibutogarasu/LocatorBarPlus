{
  "schemaVersion": 1,
  "id": "locatorbarplus",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/locatorbarplus/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.karasu256.locatorbarplus.client.LocatorBarPlusDataGenerator"
    ],
    "client": [
      "com.karasu256.locatorbarplus.client.LocatorBarPlusClient"
    ],
    "modmenu": [
      "com.karasu256.locatorbarplus.client.LocatorBarPlusModMenuIntegration"
    ]
  },
  "mixins": [
    "locatorbarplus.mixins.json",
    {
      "config": "locatorbarplus.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_329": ["com/karasu256/locatorbarplus/impl/IInGameHud"],
      "net/minecraft/class_11226": ["com/karasu256/locatorbarplus/impl/ILocatorBar"],
      "net/minecraft/class_11224": ["com/karasu256/locatorbarplus/impl/IExperienceBar"]
    }
  }
